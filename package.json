{
  "name": "express-nodejs-template",
  "version": "1.0.0",
  "description": "Starter template for designing nodeJS  api using expressjs",
  "main": "src/index.js",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.3",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.32.0",
    "ghooks": "^2.0.4",
    "jest": "^27.0.6",
    "mongodb-memory-server": "^7.3.4",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production npm run build && node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/server.ts",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "watch:ts": "tsc --watch",
    "lint": "eslint \"src/**/*.ts\" --quiet --fix",
    "format": "prettier --write  \"src/**/*.ts\"",
    "test": "cross-env NODE_ENV=testing jest --forceExit --setupFiles dotenv/config --detectOpenHandles --testTimout=10001",
    "test:controllers": "npm run test -t controllers",
    "test:middlewares": "npm run test -t middlewares",
    "test:utils": "npm run test -t utils",
    "test:routes": "npm run test -t routes",
    "test:models": "npm run test -t models",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saurabhmehta1601/nodejs-express-template.git"
  },
  "keywords": [],
  "author": "Saurabh Mehta <saurabhmehta1601@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saurabhmehta1601/nodejs-express-template/issues"
  },
  "homepage": "https://github.com/saurabhmehta1601/nodejs-express-template#readme",
  "config": {
    "ghooks": {
      "pre-commit": "npm-run-all --parallel lint format"
    }
  }
}
